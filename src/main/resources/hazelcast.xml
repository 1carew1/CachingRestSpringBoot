<?xml version="1.0" encoding="UTF-8"?>
<hazelcast
        xsi:schemaLocation="http://www.hazelcast.com/schema/config http://www.hazelcast.com/schema/config/hazelcast-config.xsd"
        xmlns="http://www.hazelcast.com/schema/config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <!--
        This is important URL : http://docs.hazelcast.org/docs/latest-development/manual/html/Hazelcast_JCache/Hazelcast_JCache_Extension-ICache/Configuring_ICache.html
        This is default URL for cache config stuff http://docs.hazelcast.org/docs/latest-development/manual/html/Hazelcast_JCache/Setup_and_Configuration/Configuring_for_JCache.html
    -->

    <cache name="imageClassifications">
        <eviction size="50" max-size-policy="ENTRY_COUNT" eviction-policy="LRU" />
    </cache>

</hazelcast>